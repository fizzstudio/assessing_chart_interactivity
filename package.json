{
  "name": "assessing-chart-interactivity",
  "author": "Frank Elavsky",
  "version": "0.0.1",
  "keywords": [
    "visualization",
    "accessibility"
  ],
  "description": "A repository for experimental environments where we intend to test assistive technology user experience with complex static and interactive charts and graphs.",
  "engines": {
    "npm": "^6.14.16",
    "node": "^14.19.1"
  },
  "scripts": {
    "prettier-all-check": "prettier --config  ./.prettierrc --ignore ./.prettierignore --debug-check \"**/*.{js,jsx,ts,tsx,html,jsx,json,css,scss,md}\"",
    "prettier-all": "prettier --config  ./.prettierrc --ignore ./.prettierignore --write \"**/*.{js,jsx,ts,tsx,html,jsx,json,css,scss,md}\""
  },
  "dependencies": {
    "@visa/charts": "^6.1.0",
    "prettier": "^2.6.2"
  }
}
